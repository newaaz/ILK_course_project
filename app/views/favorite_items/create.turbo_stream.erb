  <%= turbo_stream.update("add_favorite_#{dom_id @listing}", partial: 'shared/add_favorite', locals: { listing: @listing }) %>


  <%#= turbo_stream.update('flash_messages', partial: 'shared/flash_message',
                                            locals: { 
                                                      message_type: 'success',
                                                      message: @status
                                                    }) %>

<% end %>

