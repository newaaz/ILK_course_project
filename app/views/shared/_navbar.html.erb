<%= link_to "Home", root_path, class: 'btn btn-outline-primary' %>
<%= link_to "Login owner", new_partner_session_path, class: 'btn btn-outline-primary' %>
<%= link_to "Login customer", new_customer_session_path, class: 'btn btn-outline-success' %>
<%= link_to "Properties", properties_path, class: 'btn btn-outline-primary' %>

<% if current_partner %>
  <%= link_to "Dashboard Partner", partners_root_path, class: 'btn btn-primary' %>
  <%= link_to "Logout Partner", destroy_partner_session_path, data: { "turbo-method": :delete }, class: 'btn btn-outline-danger' %>
<% end %>

<% if current_customer %>
  <%= link_to "Dashboard Customer", customers_root_path, class: 'btn btn-primary' %>
  <%= link_to "Logout Customer", destroy_customer_session_path, data: { "turbo-method": :delete }, class: 'btn btn-outline-danger' %>
<% end %>