<div class="col-6">
  <%= form_with(model: [:admin, @category], data: {turbo: false}) do |f| %>
    <%= render 'shared/errors', object: f.object%>      
    <div class="form-group">
      <%= f.label :title %>        
      <%= f.text_field :title, class:"form-control" %>
    </div>
    
    <div class="form-group">
      <%= f.label :number %>        
      <%= f.number_field :number, class:"form-control" %>
    </div>

    <%= f.submit "Save",class:"btn btn-primary my-4" %>
  <% end %>
</div>
