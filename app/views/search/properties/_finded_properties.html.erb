<div id="search_result">
  <% if @properties.any? %>
    <h3><%= "Found #{@properties.count} property" %></h3>
    <div class="row gx-2">
      <% if session['check_in'] && session['check_out'] %>
        <%= render partial: 'search/properties/property_with_date', collection: @properties, as: :property %>
      <% else %>
        <%= render partial: 'search/properties/property_without_date', collection: @properties, as: :property %>
      <% end %>      
    </div>
  <% else %>
    <h3>Not found</h3>
  <% end %>
</div> 

